{
	"extends": "@tsconfig/strictest/tsconfig.json",
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"types": ["vite/client"],
		"importsNotUsedAsValues": "remove",
		"baseUrl": "./",
		"paths": {
			"~/*": ["./src/*"]
		}
	}
}
